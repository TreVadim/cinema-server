{
	"name": "server",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "node index.js",
		"server": "nodemon index.js"
	},
	"eslintConfig": {
		"extends": "airbnb-base",
		"env": {
			"es6": true,
			"browser": true
		},
		"rules": {
			"brace-style": [
				"error",
				"stroustrup"
			],
			"comma-dangle": [
				"error",
				"never"
			],
			"no-unused-vars": [
				"warn"
			],
			"no-underscore-dangle": 0,
			"no-var": [
				"off"
			],
			"one-var": [
				"off"
			],
			"indent": [
				2,
				"tab"
			],
			"no-tabs": 0,
			"camelcase": 0
		}
	},
	"author": "Entony",
	"license": "ISC",
	"dependencies": {
		"axios": "^0.19.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.18.3",
		"cors": "^2.8.5",
		"express": "^4.16.4",
		"express-fileupload": "^1.1.1-alpha.1",
		"image-size": "^0.7.4",
		"jsonwebtoken": "^9.0.0",
		"mongoose": "^5.4.3",
		"nodemon": "^1.18.9",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"swagger-ui-express": "^4.0.3"
	},
	"devDependencies": {
		"eslint": "^5.12.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-import": "^2.14.0"
	}
}
